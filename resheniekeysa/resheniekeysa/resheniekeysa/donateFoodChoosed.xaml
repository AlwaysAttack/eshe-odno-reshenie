<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="resheniekeysa.donateFoodChoosed">
    <ContentPage.Content>
        <StackLayout Margin="10">
            <Entry x:Name="PhoneNumber"
           Placeholder="Номер телефона"></Entry>
            <Entry x:Name="NameHelper" Placeholder="Ваше имя"/>
            <Entry x:Name="Quent" Placeholder="Количество корма в кг"/>
            
            <Button x:Name="sendREQUESTdonateFood" Text="Отправить запрос" BackgroundColor="#65AC64" CornerRadius="20" Clicked="sendREQUESTdonateFood_Clicked"/>
        </StackLayout>
    </ContentPage.Content>
</ContentPage>